@model WebStore.Models.BindSelect
@{
    ViewBag.Title = WebStore.Resources.Titles.CheckOut;
}

<section>
    <header class="header">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <h1>
                        @ViewBag.Title
                    </h1>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <ul class="breadcrumbs">
                        <li>
                            <a href="@Url.Action("Index","Home", new { language = System.Threading.Thread.CurrentThread.CurrentUICulture.TwoLetterISOLanguageName })">@WebStore.Resources.Titles.Home</a>
                        </li>
                        <li class="separator">&nbsp;</li>
                        <li>
                            <a href="@Url.Action("Cart","User", new { language = System.Threading.Thread.CurrentThread.CurrentUICulture.TwoLetterISOLanguageName})">@WebStore.Resources.Titles.Cart</a>
                        </li>
                        <li class="separator">&nbsp;</li>
                        <li>
                            @ViewBag.Title
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
</section>

<div class="container padding-bottom-3x mb-2">
    <div class="row">
        <div class="col-lg-8 col-xl-9">
            <form id="CheckOutSteps" action="#" novalidate="novalidate">
                <h3>
                    <span class="step-Title">@WebStore.Resources.Check.Data</span>
                    <span class="fas fa-address-card fa-2x pl-3"></span>
                </h3>
                <fieldset>
                    <div class="row">
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-country">@WebStore.Resources.Placeholder.DocumentType</label>
                            <div class="form-group input-group">
                                <select class="form-control" required autocomplete="off" name="checkout-document" id="checkout-country">
                                    <option value="">@WebStore.Resources.Placeholder.DocumentType</option>
                                    <option value="1">Factura</option>
                                    <option value="2">Boleta</option>
                                </select>
                                <span class="input-group-addon">
                                    <i class="fas fa-file"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="font-weight-bold pl-3">
                                @WebStore.Resources.Placeholder.IdNum
                                <small>
                                    <a href="javascript:void(0)" class="text-orangered" id="popover" data-toggle="popover" data-trigger="hover" data-placement="right" title="Número de ID" data-content="Los paquetes no serán entregados sin el número válido de identificación del cliente. Nosotros no usamos esta información para algún otro propósito. Por favor ingrese su número de identificación.">
                                        Leer más <i class="fas fa-angle-down"></i>
                                    </a>
                                </small>
                            </label>
                            <div class="form-group input-group">
                                <input class="form-control" type="text" name="checkout-id" id="checkout-id" placeholder="@WebStore.Resources.Placeholder.IdNum" />
                                <span class="input-group-addon">
                                    <i class="fas fa-id-card"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-fn">@WebStore.Resources.Placeholder.Names</label>
                            <div class="form-group input-group">
                                <input class="form-control" required autocomplete="off" type="text" name="checkout-fn" id="checkout-fn" placeholder="@WebStore.Resources.Placeholder.Names">
                                <span class="input-group-addon">
                                    <i class="fas fa-file-signature"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-ln">@WebStore.Resources.Placeholder.LastNames</label>
                            <div class="form-group input-group">
                                <input class="form-control" required autocomplete="off" type="text" name="checkout-ln" id="checkout-ln" placeholder="@WebStore.Resources.Placeholder.LastNames">
                                <span class="input-group-addon">
                                    <i class="fas fa-file-signature"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-email">@WebStore.Resources.Placeholder.Email</label>
                            <div class="form-group input-group">
                                <input class="form-control" required autocomplete="off" type="email" name="checkout-email" id="checkout-email" placeholder="@WebStore.Resources.Placeholder.Email">
                                <span class="input-group-addon">
                                    <i class="fas fa-at"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-phone">@WebStore.Resources.Placeholder.Telephone</label>
                            <div class="form-group input-group">
                                <input class="form-control" required autocomplete="off" type="text" name="checkout-phone" id="checkout-phone" placeholder="@WebStore.Resources.Placeholder.Phone">
                                <span class="input-group-addon">
                                    <i class="fas fa-mobile-alt"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-company">@WebStore.Resources.Placeholder.Company</label>
                            <div class="form-group input-group">
                                <input class="form-control" required autocomplete="off" type="text" name="checkout-company" id="checkout-company" placeholder="@WebStore.Resources.Placeholder.Company">
                                <span class="input-group-addon">
                                    <i class="fas fa-industry"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="font-weight-bold pl-3">@WebStore.Resources.Placeholder.Region</label>
                            <div class="form-group input-group">
                                <select class="form-control" type="text" name="checkout-states" id="strStatesNationale" placeholder="@WebStore.Resources.Placeholder.Region">
                                    <option value="">@WebStore.Resources.Placeholder.Region</option>
                                    @foreach (var region in Model.regions)
                                    {
                                        <option value="@Html.DisplayFor(a => region.idRegion)">@Html.DisplayFor(a => region.strNombre) @Html.DisplayFor(a => region.intNumber)</option>
                                    }
                                </select>
                                <span class="input-group-addon">
                                    <i class="fas fa-flag"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <label class="font-weight-bold pl-3">@WebStore.Resources.Placeholder.Province</label>
                            <div class="form-group input-group">
                                <select class="form-control" type="text" name="checkout-provinces" id="strProvinciaNationale" placeholder="@WebStore.Resources.Placeholder.Province">
                                    <option value="">@WebStore.Resources.Placeholder.Province</option>
                                </select>
                                <span class="input-group-addon">
                                    <i class="fas fa-city"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="font-weight-bold pl-3">@WebStore.Resources.Placeholder.Comune</label>
                            <div class="form-group input-group">
                                <select class="form-control" type="text" name="checkout-comunes" id="strComunaNationale" placeholder="@WebStore.Resources.Placeholder.Comune">
                                    <option value="">@WebStore.Resources.Placeholder.Comune</option>
                                </select>
                                <span class="input-group-addon">
                                    <i class="far fa-object-group"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-address1">@WebStore.Resources.Placeholder.Address 1</label>
                            <div class="form-group input-group">
                                <input class="form-control" type="text" name="checkout-address1" id="checkout-address1" required autocomplete="off" placeholder="@WebStore.Resources.Placeholder.Address 1">
                                <span class="input-group-addon">
                                    <i class="fas fa-map-marker-alt"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-address2">@WebStore.Resources.Placeholder.Address 2</label>
                            <div class="form-group input-group">
                                <input class="form-control" type="text" name="checkout-address2" id="checkout-address2" required autocomplete="off" placeholder="@WebStore.Resources.Placeholder.Address 2">
                                <span class="input-group-addon">
                                    <i class="fas fa-map-marker-alt"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class=" row padding-bottom-1x">
                        <div class="col-sm-6">
                            <label class="font-weight-bold" for="checkout-zip">@WebStore.Resources.Placeholder.PostCode</label>
                            <div class="form-group input-group">
                                <input class="form-control" required autocomplete="off" type="text" name="checkout-zip" id="checkout-zip" placeholder="@WebStore.Resources.Placeholder.PostCode">
                                <span class="input-group-addon">
                                    <i class="fas fa-map-marked-alt"></i>
                                </span>
                            </div>
                        </div>
                    </div>

                    <h4>@WebStore.Resources.Titles.ShippingType</h4>
                    <hr class="padding-bottom-1x">
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <input type="radio" class="radio with-gap radio-col-deep-orange" name="strShippingType" id="store" value="1" />
                                <label for="store">@WebStore.Resources.Placeholder.Store</label>
                                <input type="radio" class="radio with-gap radio-col-deep-orange" name="strShippingType" id="shipping" value="2" />
                                <label for="shipping">@WebStore.Resources.Placeholder.Shipping</label>
                            </div>
                        </div>
                    </div>
                </fieldset>
                <h3>
                    <span class="step-Title">@WebStore.Resources.Check.Shipping</span>
                    <span class="fas fa-truck-moving fa-2x pl-3"></span>
                </h3>
                <fieldset>
                    <div id="ShippingTypeStore" style="display:none;">
                        <h6>@WebStore.Resources.Placeholder.Store</h6>
                        <hr />
                        <p>@WebStore.Resources.Texts.Address: Presidente Eduardo Frei Montalva N°3585 </p>
                    </div>
                    <div id="ShippingTypeAddress" style="display: none;">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input class="chk-col-deep-orange filled-in" name="sameAddress" type="checkbox" id="same_address" checked="">
                                <label for="same_address">@WebStore.Resources.Placeholder.SameAddress</label>
                            </div>
                        </div>
                        <div class="form-group pb-2">
                            <div class="custom-control custom-checkbox">
                                <input class="chk-col-deep-orange filled-in" name="samePerson" type="checkbox" id="same_person" checked="">
                                <label for="same_person">@WebStore.Resources.Placeholder.SamePerson</label>
                            </div>
                        </div>

                        <div class="d-none pb-3" id="anotherPerson">
                            <h6>@WebStore.Resources.Titles.AnotherPersonInfo</h6>
                            <hr class="padding-bottom-1x">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="font-weight-bold" for="checkout-fn">@WebStore.Resources.Placeholder.Names</label>
                                    <div class="form-group input-group">
                                        <input class="form-control" autocomplete="off" type="text" name="anotherPersonName" id="checkout-fn" placeholder="@WebStore.Resources.Placeholder.Names">
                                        <span class="input-group-addon">
                                            <i class="fas fa-file-signature"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label class="font-weight-bold" for="checkout-ln">@WebStore.Resources.Placeholder.LastNames</label>
                                    <div class="form-group input-group">
                                        <input class="form-control" autocomplete="off" type="text" name="anotherPersonLastName" id="checkout-ln" placeholder="@WebStore.Resources.Placeholder.LastNames">
                                        <span class="input-group-addon">
                                            <i class="fas fa-file-signature"></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="font-weight-bold pl-3">
                                        @WebStore.Resources.Placeholder.IdNum
                                        <small>
                                            <a href="javascript:void(0)" class="text-orangered" id="popover" data-toggle="popover" data-trigger="hover" data-placement="right" title="Número de ID" data-content="Los paquetes no serán entregados sin el número válido de identificación del cliente. Nosotros no usamos esta información para algún otro propósito. Por favor ingrese su número de identificación.">
                                                Leer más <i class="fas fa-angle-down"></i>
                                            </a>
                                        </small>
                                    </label>
                                    <div class="form-group input-group">
                                        <input class="form-control" type="text" name="anotherPersonId" id="anotherPersonId" placeholder="@WebStore.Resources.Placeholder.IdNum" />
                                        <span class="input-group-addon">
                                            <i class="fas fa-id-card"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label class="font-weight-bold" for="checkout-phone">@WebStore.Resources.Placeholder.Telephone</label>
                                    <div class="form-group input-group">
                                        <input class="form-control" required autocomplete="off" type="text" name="checkout-phone" id="anotherPersonPhone" placeholder="@WebStore.Resources.Placeholder.Phone">
                                        <span class="input-group-addon">
                                            <i class="fas fa-mobile-alt"></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="d-none" id="anotherAddress">
                            <h6>@WebStore.Resources.Titles.AnotherAddressInfo</h6>
                            <hr class="padding-bottom-1x">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="font-weight-bold pl-3">@WebStore.Resources.Placeholder.Region</label>
                                    <div class="form-group input-group">
                                        <select class="form-control" type="text" name="checkout-Anotherstates" id="strStatesNationaleAnother" placeholder="@WebStore.Resources.Placeholder.Region">
                                            <option value="">@WebStore.Resources.Placeholder.Region</option>
                                            @foreach (var region in Model.regions)
                                            {
                                                <option value="@Html.DisplayFor(a => region.idRegion)">@Html.DisplayFor(a => region.strNombre) @Html.DisplayFor(a => region.intNumber)</option>
                                            }
                                        </select>
                                        <span class="input-group-addon">
                                            <i class="fas fa-flag"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label class="font-weight-bold pl-3">@WebStore.Resources.Placeholder.Province</label>
                                    <div class="form-group input-group">
                                        <select class="form-control" type="text" name="checkout-Anotherprovinces" id="strProvinciaNationaleAnother" placeholder="@WebStore.Resources.Placeholder.Province">
                                            <option value="">@WebStore.Resources.Placeholder.Province</option>
                                        </select>
                                        <span class="input-group-addon">
                                            <i class="fas fa-city"></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="font-weight-bold pl-3">@WebStore.Resources.Placeholder.Comune</label>
                                    <div class="form-group input-group">
                                        <select class="form-control" type="text" name="checkout-Anothercomunes" id="strComunaNationaleAnother" placeholder="@WebStore.Resources.Placeholder.Comune">
                                            <option value="">@WebStore.Resources.Placeholder.Comune</option>
                                        </select>
                                        <span class="input-group-addon">
                                            <i class="far fa-object-group"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label class="font-weight-bold" for="checkout-address1">@WebStore.Resources.Placeholder.Address 1</label>
                                    <div class="form-group input-group">
                                        <input class="form-control" type="text" name="checkout-Anotheraddress1" id="checkout-address1" required autocomplete="off" placeholder="@WebStore.Resources.Placeholder.Address 1">
                                        <span class="input-group-addon">
                                            <i class="fas fa-map-marker-alt"></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row padding-bottom-1x">
                                <div class="col-sm-6">
                                    <label class="font-weight-bold" for="checkout-address2">@WebStore.Resources.Placeholder.Address 2</label>
                                    <div class="form-group input-group">
                                        <input class="form-control" type="text" name="checkout-Anotheraddress2" id="checkout-address2" required autocomplete="off" placeholder="@WebStore.Resources.Placeholder.Address 2">
                                        <span class="input-group-addon">
                                            <i class="fas fa-map-marker-alt"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label class="font-weight-bold" for="checkout-zip">@WebStore.Resources.Placeholder.PostCode</label>
                                    <div class="form-group input-group">
                                        <input class="form-control" required autocomplete="off" type="text" name="checkout-zip" id="checkout-zip" placeholder="@WebStore.Resources.Placeholder.PostCode">
                                        <span class="input-group-addon">
                                            <i class="fas fa-map-marked-alt"></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead class="thead-default">
                                    <tr>
                                        <th></th>
                                        <th>@WebStore.Resources.Check.ShippingM</th>
                                        <th>@WebStore.Resources.Check.DeliveryTime</th>
                                        <th>@WebStore.Resources.Check.Fee</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="courier" name="shipping-method"
                                                       checked="">
                                                <label for="courier"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-dark">Courier</span><br><span class="text-muted text-sm">
                                                All
                                                Addresses(default zone), United States &amp; Canada
                                            </span>
                                        </td>
                                        <td class="align-middle">2 - 4 days</td>
                                        <td class="align-middle">$26.50</td>
                                    </tr>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="local" name="shipping-method">
                                                <label for="local"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-dark">Local Shipping</span><br><span class="text-muted text-sm">
                                                All
                                                Addresses(default zone), United States &amp; Canada
                                            </span>
                                        </td>
                                        <td class="align-middle">up to one week</td>
                                        <td class="align-middle">$10.00</td>
                                    </tr>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="flat" name="shipping-method">
                                                <label for="flat"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-darkm">Flat Rate</span><br><span class="text-muted text-sm">
                                                All
                                                Addresses(default zone)
                                            </span>
                                        </td>
                                        <td class="align-middle">5 - 7 days</td>
                                        <td class="align-middle">$33.85</td>
                                    </tr>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="ups" name="shipping-method">
                                                <label for="ups"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-dark">UPS Ground Shipping</span><br><span class="text-muted text-sm">
                                                All
                                                Addresses(default zone)
                                            </span>
                                        </td>
                                        <td class="align-middle">4 - 6 days</td>
                                        <td class="align-middle">$18.00</td>
                                    </tr>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="pickup" name="shipping-method">
                                                <label for="pickup"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-dark">Local Pickup from store</span><br><span class="text-muted text-sm">All Addresses(default zone)</span>
                                        </td>
                                        <td class="align-middle">—</td>
                                        <td class="align-middle">$0.00</td>
                                    </tr>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="locker" name="shipping-method">
                                                <label for="locker"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-dark">Pick Up at Unishop Locker</span><br><span class="text-muted text-sm">All Addresses(default zone), United States &amp; Canada</span>
                                        </td>
                                        <td class="align-middle">—</td>
                                        <td class="align-middle">$9.99</td>
                                    </tr>
                                    <tr>
                                        <td class="align-middle">
                                            <div class="custom-control custom-radio mb-0">
                                                <input class="radio with-gap radio-col-deep-orange" type="radio" id="global" name="shipping-method">
                                                <label for="global"></label>
                                            </div>
                                        </td>
                                        <td class="align-middle">
                                            <span class="text-gray-dark">Unishop Global Export</span><br><span class="text-muted text-sm">All Addresses(default zone), outside United States</span>
                                        </td>
                                        <td class="align-middle">3 - 4 days;</td>
                                        <td class="align-middle">$25.00</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </fieldset>
                <h3>
                    <span class="step-Title">@WebStore.Resources.Check.Payment</span>
                    <span class="fas fa-money-bill-alt fa-2x pl-3"></span>
                </h3>
                <fieldset>
                    <div class="cc-selector">
                        <input id="webpay" type="radio" name="credit-card" value="webpay" required="">
                        <label class="drinkcard-cc webpay" for="webpay"></label>
                        <input id="transferencia" type="radio" name="credit-card" value="transferencia" required="">
                        <label class="drinkcard-cc transferencia" for="transferencia"></label>
                    </div>
                </fieldset>
                <h3>
                    <span class="step-Title">@WebStore.Resources.Check.Resume</span>
                    <span class="fas fa-clipboard-list fa-2x pl-3"></span>
                </h3>
                <fieldset>
                    <div class="table-responsive shopping-cart">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>@WebStore.Resources.Check.Product</th>
                                    <th class="text-center">@WebStore.Resources.Check.Subtotal</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        <div class="product-item">
                                            <a class="product-thumb" href="shop-single.html">
                                                <img src="http://themes.rokaux.com/unishop/v3.0/template-3/img/shop/cart/01.jpg" alt="Product">
                                            </a>
                                            <div class="product-info">
                                                <h4 class="product-title">
                                                    <a href="shop-single.html">
                                                        Canon EOS M50 Mirrorless
                                                        Camera<small>x 1</small>
                                                    </a>
                                                </h4>
                                                <span>
                                                    <em>@WebStore.Resources.Cart.Type:</em> Mirrorless
